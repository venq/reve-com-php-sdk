{
    "name": "venq/reve-com-php-sdk",
    "description": "Unofficial PHP SDK for Reve API (generations, edits, remix)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.1",
        "psr/log": "^3.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "phpstan/phpstan": "^1.11",
        "squizlabs/php_codesniffer": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Reve\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reve\\SDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "stan": "phpstan analyse",
        "lint": "phpcs --standard=PSR12 src tests"
    },
    "config": {
        "sort-packages": true
    }
}

